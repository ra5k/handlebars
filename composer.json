{
    "name": "ra5k/handlebars",
    "description": "A Handlebars implementation written in PHP",
    "license": "BSD-3-Clause",
    "keywords": ["library", "php", "handlears", "mustache", "templating", "oop"],
    "homepage": "https://github.com/ra5k/handlebars",
    "require": {
        "php": ">=7.0",
        "psr/log": "~1",
        "psr/cache": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "symfony/yaml": "~3",
        "symfony/cache": "~3"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Ra5k\\Handlebars\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ra5k\\Handlebars\\Test\\": "test/src/"
        }
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit --bootstrap test/bootstrap.php test/src"
    }
}
